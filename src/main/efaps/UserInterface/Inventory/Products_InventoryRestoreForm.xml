    <definition>
        <version-expression>(version==latest)</version-expression>
        <name>Products_InventoryRestoreForm</name>
        <field name="date">
            <property name="Attribute">Date</property>
            <property name="ModeCreate">EDITABLE</property>
            <property name="Required">true</property>
            <property name="ClassNameUI">org.efaps.admin.datamodel.ui.DateUI</property>
            <property name="Label">Products_InventoryRestoreForm.date.Label</property>
        </field>
        <field name="product">
            <property name="Attribute">Product</property>
            <property name="ModeCreate">EDITABLE</property>
            <property name="Required">true</property>
            <property name="Label">Products_InventoryRestoreForm.product.Label</property>
            <trigger program="org.efaps.esjp.products.Product"
                    method="autoComplete4ProductsInStorage"
                    name="Products_InventoryMoveForm.product.autocomplete"
                    event="UI_FIELD_AUTOCOMPLETE" />
        </field>
        <field name="quantity">
            <property name="Attribute">Quantity</property>
            <property name="ModeCreate">EDITABLE</property>
            <property name="Required">true</property>
            <property name="Label">Products_InventoryRestoreForm.quantity.Label</property>
        </field>
        <field name="uoM">
            <property name="ModeCreate">EDITABLE</property>
            <property name="Attribute">UoM</property>
            <property name="Label">Products_InventoryRestoreForm.uoM.Label</property>
        </field>
    </definition>
